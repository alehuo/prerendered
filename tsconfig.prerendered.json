{
  "extends": "./tsconfig.json",
  "lib": [
    "es2015",
    "dom"
  ],
  "rootDir": "./src",
  "tsBuildInfoFile": "./tsconfig.prerendered.tsbuildinfo",
  "compilerOptions": {
    "baseUrl": "./src/prerendered",
    "sourceMap": true,
    "declaration": true,
    "declarationMap": true,
    "declarationDir": "./dist/lib",
    "outDir": "./dist/lib",
  },
  "include": [
    "src/prerendered/**/*.ts",
    "src/prerendered/**/*.tsx",
  ],
}